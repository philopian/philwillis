{
  "name": "markdown-blog-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 8080",
    "start": "next dev -p 8080",
    "build": "next build && next export -o dist",
    "serve": "npm run build && serve out",
    "lint": "prettier --write {components,pages}/**/*.tsx {components,pages}/*.tsx utils/**/*.ts",
    "new:component": "node tools/new-component.js",
    "test": "vitest",
    "test:once": "vitest --run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:coverage:serve": "PORT=3003 npx live-server coverage",
    "test:coverage:watch": "npm-run-all --parallel test:coverage:serve test:coverage",
    "test:e2e": "playwright test",
    "test:e2e:debug": "PWDEBUG=1 playwright test --project=chromium",
    "test:e2e:headed": "playwright test --headed",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@heroicons/react": "^1.0.6",
    "classnames": "^2.3.1",
    "next": "12.1.6",
    "prismjs": "^1.28.0",
    "react": "18.1.0",
    "react-dom": "18.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.0",
    "@headlessui/react": "^1.6.2",
    "@playwright/test": "^1.22.1",
    "@storybook/addon-actions": "^6.5.3",
    "@storybook/addon-essentials": "^6.5.3",
    "@storybook/addon-interactions": "^6.5.3",
    "@storybook/addon-links": "^6.5.3",
    "@storybook/builder-webpack5": "^6.5.3",
    "@storybook/manager-webpack5": "^6.5.3",
    "@storybook/react": "^6.5.3",
    "@storybook/testing-library": "^0.0.11",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/react-hooks": "^8.0.0",
    "@testing-library/user-event": "^14.2.0",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/node": "17.0.35",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "@vitejs/plugin-react": "^1.3.2",
    "@vitest/ui": "^0.12.7",
    "babel-loader": "^8.2.5",
    "c8": "^7.11.3",
    "eslint": "8.15.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "gray-matter": "^4.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^19.0.0",
    "mdx-bundler": "^9.0.1",
    "next-themes": "^0.2.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-code-titles": "^1.0.3",
    "rehype-prism-plus": "^1.4.0",
    "rehype-slug": "^5.0.1",
    "serve": "^13.0.2",
    "typescript": "4.6.4",
    "vitest": "^0.12.7"
  }
}
